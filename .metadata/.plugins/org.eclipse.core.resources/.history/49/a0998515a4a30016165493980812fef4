package com.subrat.linkedList;

/**
 * @author sparida
 *
 */
public class CloneALinkedListPractice {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		

	}
	
	private static class Node{
		private Integer data;
		private Node next;
		private Node random;
		
		public Node(Integer data){
			this.data=data;
		}

		@Override
		public String toString() {
			return "[prev="+getNullOrValue(this.random)+",data="+getNullOrValue(this)+",next="+getNullOrValue(this.next)+"]";
		}

		private String getNullOrValue(Node node) {
			return (node.data!=null?node.data:null).toString();
			
		}
		
		
	}

}
